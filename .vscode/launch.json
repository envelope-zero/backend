{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run API (SQLite)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "GIN_MODE": "debug",
        "API_URL": "http://localhost:8080",
        "CORS_ALLOW_ORIGINS": "http://localhost:3000"
      },
      "console": "integratedTerminal"
    }
  ]
}
